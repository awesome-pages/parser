{
  "name": "awesome-pages",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "vitest run",
    "dev:test": "vitest",
    "parse": "tsx src/cli.ts examples/awesome-click-and-use.md readme.domain.json",
